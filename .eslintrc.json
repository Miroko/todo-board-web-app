{
    "rules": {
      "no-unused-vars": 1,
      "no-console": 0,
      "quotes": [ 1, "double" ],
      "linebreak-style": [ 1, "unix" ],
      "semi": [ 2, "always" ]
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    },
    "plugins": [
      "react"
    ]
}
